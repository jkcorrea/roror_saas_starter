    "@rails/activestorage": "^6.0.0",
    "@rails/ujs": "^6.0.0",
    "@rails/webpacker": "5.1.1",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "js-yaml": "^3.14.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-on-rails": "12.0.1",
    "react_ujs": "^2.6.1",
    "ts-loader": "^8.0.1",
    "turbolinks": "^5.2.0",
